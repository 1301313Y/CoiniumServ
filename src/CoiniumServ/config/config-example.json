{
    "stack": {
        "name": "CoiniumServ.com",
        "nodes": [
            {
                "location": "eu",
                "address": "127.0.0.1",
                "default": true
            }
        ]
    },
    "statistics": {
        "updateInterval": 60,
        "hashrateWindow": 300
    },
    "website": {
        "enabled": true,
        "bind": "",
        "port": 80,
        "template": "default",
        "social": {
            "rss": "http://bit.ly/1qheogE",
            "twitter": "https://twitter.com/coinium",
            "facebook": "https://www.facebook.com/pages/Coiniumorg/639179226128343",
            "googleplus": "https://plus.google.com/+CoiniumOrgNetwork/about",
            "youtube": "http://www.youtube.com/user/CoiniumNetwork/"
        },
        "backend": {
            "metrics": {
                "enabled": false
            }
        }
    },
    "logging": {
        "root": "logs",
        "targets": [
            {
                "enabled": true,
                "type": "file",
                "filename": "server.log",
                "rolling": false,
                "level": "information"
            },
            {
                "enabled": true,
                "type": "file",
                "filename": "debug.log",
                "rolling": false,
                "level": "verbose"
            },
            {
                "enabled": false,
                "type": "packet",
                "filename": "packet.log",
                "rolling": false,
                "level": "verbose"
            }
        ]
    }
}